 "html": "\n<img src='/files/Ethal Letter head New575619.png' style='height: 80px;'>\n<br>\n<br>\n<table><tr><td><b>Name :</b> {{doc.name}}</td><td><b>Date :</b> {{frappe.utils.get_datetime(doc.posting_date).strftime('%d-%m-%Y')}}</td></tr></table>\n<br>\n<br>\n{% if doc.shifts %}\n<table border=\"1\" cellpadding=\"5\" cellspacing=\"0\">\n  <tr>\n    <th rowspan=\"1\" style=\"text-align:center;\">SHIFT</th>\n    <th colspan=\"3\" style=\"text-align:center;\">A</th>\n    <th colspan=\"3\" style=\"text-align:center;\">B</th>\n    <th colspan=\"3\" style=\"text-align:center;\">C</th>\n    <th colspan=\"3\" style=\"text-align:center;\">TOTAL</th>\n  </tr>\n  <tr>\n    <th style=\"text-align:center;\">INGOT</th>\n    <th style=\"text-align:center;\">No</th>\n    <th style=\"text-align:center;\">kg</th>\n    <th style=\"text-align:center;\">Heat</th>\n    <th style=\"text-align:center;\">No</th>\n    <th style=\"text-align:center;\">kg</th>\n    <th style=\"text-align:center;\">Heat</th>\n    <th style=\"text-align:center;\">No</th>\n    <th style=\"text-align:center;\">kg</th>\n    <th style=\"text-align:center;\">Heat</th>\n    <th style=\"text-align:center;\">No</th>\n    <th style=\"text-align:center;\">kg</th>\n    <th style=\"text-align:center;\">Heat</th>\n  </tr>\n  \n  {% set ingot_lst = [\"MF- 1 (Oil)\",\"MF- 2 (Oil)\",\"MF- 3 (Oil)\",\"MF- 4 (Oil)\",\"MF- 5 (Oil)\"] %}\n \n  {% for ig in ingot_lst %}\n  <tr>\n    <td>{{ig}}</td>\n    {% set ns = namespace (newtotal_no = 0) %}\n    {% set kg = namespace (newtotal_kg = 0) %}\n    {% set heat = namespace (newtotal_heat = 0) %}\n    {% for row in doc.shifts %}\n        {% if row.ingot == ig and (row.shift == \"A\" or row.shift == \"B\" or row.shift == \"C\") %}\n            <td style=\"text-align:Right;\">{{ row.no }}</td>\n            <td style=\"text-align:Right;\">{{ row.kg }}</td>\n            <td style=\"text-align:Right;\">{{ row.heat }}</td>\n            {% set ns.newtotal_no = ns.newtotal_no + row.no|int %}\n            {% set kg.newtotal_kg = kg.newtotal_kg + row.kg|int %}\n            {% set heat.newtotal_heat = heat.newtotal_heat + row.heat|int %}\n        {% endif %}\n  \n    {% endfor %}\n    <td style=\"text-align:Right;\">{{ ns.newtotal_no }}</td>\n    <td style=\"text-align:Right;\">{{ kg.newtotal_kg }}</td>\n    <td style=\"text-align:Right;\">{{ heat.newtotal_heat }}</td>\n    \n</tr>\n{% endfor %}\n<tr>\n    <td colspan = \"10\" style=\"text-align:center;\"><b>Total</b></td>\n    <td style=\"text-align:Right;\">{{doc.total_no}}</td>\n    <td style=\"text-align:Right;\">{{doc.total_kg}}</td>\n    <td style=\"text-align:Right;\">{{doc.total_heats}}</td>\n</tr>\n</table>\n<br>\n{% endif %}\n\n{% if doc.attendance %}\n<table border=\"1\" cellpadding=\"5\" cellspacing=\"0\" text>\n    <tr><th colspan = \"5\" style=\"text-align:center;\"> ATTENDANCE </th></tr>\n    <tr>\n        <th style=\"text-align:center;\">SECTION</th>\n        <th style=\"text-align:center;\">TYPE</th>\n        <th style=\"text-align:center;\">PRESENT</th>\n        <th style=\"text-align:center;\">ABSENT</th>\n        <th style=\"text-align:center;\">OT (HOURS)</th>\n    </tr>\n    {% for i in doc.attendance %}\n     <tr>\n         <td style=\"text-align:center;\">{{i.section}}</td>\n         <td style=\"text-align:center;\">{{i.type}}</td>\n         <td style=\"text-align:Right;\">{{i.present|int}}</td>\n         <td style=\"text-align:Right;\">{{i.absent}}</td>\n         <td style=\"text-align:Right;\">{{i.ot_hours}}</td>\n     </tr>\n    {% endfor%}\n    <tr>\n        <td colspan = \"2\" style=\"text-align:center; background-color:white;\"><b>Total</b></td>\n        <td style=\"text-align:Right;\">{{doc.total_present}}</td>\n        <td style=\"text-align:Right;\">{{doc.total_absent}}</td>\n        <td style=\"text-align:Right;\">{{doc.total_ot_hours}}</td>\n    </tr>\n</table>\n<br>\n{% endif %}\n\n<table border=\"1\" cellpadding=\"5\" cellspacing=\"0\">\n  <tr>\n    <th style=\"text-align:center;\">QC</th>\n    <th style=\"text-align:center;\">kg.</th>\n    <th style=\"text-align:center;\">%</th>\n  </tr>\n  <tr>\n    <td>RECEIVED:</td>\n    <td style=\"text-align:Right;\">{{doc.received}}</td>\n    <td style=\"text-align:Right;\">-</td>\n  </tr>\n  <tr>\n    <td>GOOD:</td>\n    <td style=\"text-align:Right;\">{{doc.good}}</td>\n    <td style=\"text-align:Right;\">\n        {% if doc.received != 0 %}\n        {{'%.2f' | format(doc.good / doc.received * 100)}}%\n        {% else %}\n        {{0}}\n        {% endif %}\n    </td>\n  </tr>\n  <tr>\n    <td>REJECTION:</td>\n    <td style=\"text-align:Right;\">{{doc.rejection}}</td>\n    <td style=\"text-align:Right;\">\n        {% if doc.received != 0 %}\n        {{'%.2f' | format(doc.rejection/doc.received *100)}}%\n        {% else %}\n        {{0}}\n        {% endif %}\n    </td>\n  </tr>\n  <tr>\n    <td>MELTING:</td>\n    <td style=\"text-align:Right;\">{{doc.melting}}</td>\n    <td style=\"text-align:Right;\">\n        {% if doc.received != 0 %}\n        {{'%.2f' | format(doc.melting/doc.received *100)}}%\n        {% else %}\n        {{0}}\n        {% endif %}\n    </td>\n  </tr>\n  <tr>\n    <td>ROLLING:</td>\n    <td style=\"text-align:Right;\">{{doc.rolling}}</td>\n    <td style=\"text-align:Right;\">\n        {% if doc.received != 0 %}\n        {{'%.2f' | format(doc.rolling/doc.received *100)}}%\n        {% else %}\n        {{0}}\n        {% endif %}\n    </td>\n  </tr>\n  <tr>\n    <td>SIZING:</td>\n    <td style=\"text-align:Right;\">{{doc.sizing}}</td>\n    <td style=\"text-align:Right;\">\n        {% if doc.received != 0 %}\n        {{'%.2f' | format(doc.sizing/doc.received *100)}}%\n        {% else %}\n        {{0}}\n        {% endif %}\n    </td>\n  </tr>\n  <tr>\n    <td>Others:</td>\n    <td style=\"text-align:Right;\">{{doc.others}}</td>\n    <td style=\"text-align:Right;\">\n        {% if doc.received != 0 %}\n        {{'%.2f' | format(doc.others/doc.received *100)}}%\n        {% else %}\n        {{0}}\n        {% endif %}\n    </td>\n  </tr>\n</table>\n<br>\n\n{% if doc.manufaturing_annealing_item %}\n<table border=\"1\" cellpadding=\"5\" cellspacing=\"0\" text>\n    <tr><th colspan = \"5\" style=\"text-align:center;\"> ANNEALING </th></tr>\n    <tr>\n        <th style=\"text-align:center;\">In Time</th>\n        <th style=\"text-align:center;\">Out ime</th>\n        <th style=\"text-align:center;\">Box</th>\n        <th style=\"text-align:center;\">Kg</th>\n    </tr>\n    {% for i in doc.manufacturing_annealing_items %}\n     <tr>\n         <td style=\"text-align:center;\">{{i.in_time}}</td>\n         <td style=\"text-align:center;\">{{i.out_time}}</td>\n         <td style=\"text-align:Right;\">{{i.box_no}}</td>\n         <td style=\"text-align:Right;\">{{i.kg}}</td>\n     </tr>\n    {% endfor%}\n    <tr><td colspan = \"3\" style=\"text-align:center; background-color:white;\"><b>Total</b></td><td style=\"text-align:Right;\">{{doc.total}}</td></tr>\n</table>\n<br>\n{% endif %}\n\n{% if doc.raw_material_received %}\n<table border=\"1\" cellpadding=\"5\" cellspacing=\"0\">\n    <tr><th colspan = \"5\" style=\"text-align:center;\"> RAW MATERIAL RECEIVED </th></tr>\n    <tr>\n        <th style=\"text-align:center;\">Item Name</th>\n        <th style=\"text-align:center;\">Purchase Receipt No</th>\n        <th style=\"text-align:center;\">Supplier Name</th>\n        <th style=\"text-align:Right;\">Accepted Qty</th>\n    </tr>\n    {% for i in doc.raw_material_received %}\n     <tr>\n         <td>{{i.item_name}}</td>\n         <td>{{i.purchase_receipt_no}}</td>\n         <td>{{i.supplier_name}}</td>\n         <td style=\"text-align:Right;\">{{i.accepeted_qty}}</td>\n     </tr>\n    {% endfor%}\n</table>\n<br>\n{% endif %}\n\n{% if doc.dispatched %}\n<table border=\"1\" cellpadding=\"5\" cellspacing=\"0\">\n    <tr><th colspan = \"5\" style=\"text-align:center;\"> DISPATCHED </th></tr>\n    <tr>\n        <th style=\"text-align:center;\">Item Name</th>\n        <th style=\"text-align:center;\">Delivery Note</th>\n        <th style=\"text-align:center;\">Customer Name</th>\n        <th style=\"text-align:Right;\">Qty</th>\n        <th style=\"text-align:Right;\">Carton/cs</th>\n    </tr>\n    {% for i in doc.dispatched %}\n     <tr>\n         <td>{{i.item_name}}</td>\n         <td>{{i.delivery_note}}</td>\n         <td>{{i.customer}}</td>\n         <td style=\"text-align:Right;\">{{i.qty}}</td>\n         <td style=\"text-align:Right;\">{{i.cartonpcs}}</td>\n     </tr>\n    {% endfor%}\n</table>\n<br>\n{% endif %}\n\n\n{% if doc.stock %}\n<table border=\"1\" cellpadding=\"5\" cellspacing=\"0\">\n    <tr><th colspan = \"5\" style=\"text-align:center;\"> STOCK </th></tr>\n    <tr>\n        <th style=\"text-align:center;\">Item</th>\n        <th style=\"text-align:center;\">Opening Stock</th>\n        <th style=\"text-align:center;\">Receipt</th>\n        <th style=\"text-align:center;\">Issue</th>\n        <th style=\"text-align:center;\">Closing Stock</th>\n    </tr>\n    {% for i in doc.stock %}\n     <tr>\n         <td>{{i.item}}</td>\n         <td style=\"text-align:Right;\">{{i.opening_stock}}</td>\n         <td style=\"text-align:Right;\">{{i.receipt}}</td>\n         <td style=\"text-align:Right;\">{{i.issue}}</td>\n         <td style=\"text-align:Right;\">{{i.closing_stock}}</td>\n     </tr>\n    {% endfor%}\n    <tr>\n         <td style=\"text-align:center;\"><b>Total</b></td>\n         <td style=\"text-align:Right;\"> {{'%.2f' | format(doc.total_opening_stock)}}</td>\n         <td style=\"text-align:Right;\">{{'%.2f' | format(doc.total_receipt)}}</td>\n         <td style=\"text-align:Right;\">{{'%.2f' | format(doc.total_issue)}}</td>\n         <td style=\"text-align:Right;\">{{'%.2f' | format(doc.total_closing_stock)}}</td>\n     </tr>\n</table>\n<br>\n{% endif %}\n\n\n{% if doc.receipts_of_this_month %}\n<table border=\"1\" cellpadding=\"5\" cellspacing=\"0\">\n    <tr><th colspan = \"5\" style=\"text-align:center;\"> RECEIPT OF THIS MONTH \u2013 CUMULATIVE /SUMMARY (kg) </th></tr>\n    <tr>\n        <th style=\"text-align:center;\" >Item</th>\n        <th style=\"text-align:center;\" >Opening</th>\n        <th style=\"text-align:center;\" >Change</th>\n        <th style=\"text-align:center;\" >Closing</th>\n    </tr>\n    {% for i in doc.receipts_of_this_month %}\n     <tr>\n         <td>{{i.item}}</td>\n         <td style=\"text-align:Right;\">{{i.opening}}</td>\n         <td style=\"text-align:Right;\">{{i.change}}</td>\n         <td style=\"text-align:Right;\">{{i.closing}}</td>\n     </tr>\n    {% endfor%}\n    <tr>\n         <td style=\"text-align:center;\"><b>Total</b></td>\n         <td style=\"text-align:Right;\">{{doc.total_opening}}</td>\n         <td style=\"text-align:Right;\">{{doc.total_change}}</td>\n         <td style=\"text-align:Right;\">{{doc.total_closing}}</td>\n     </tr>\n</table>\n<br>\n{% endif %}\n<br>\n<table>\n    <tr>\n        <td class=\"text-center\" style='padding: 0px !important'>Pramod Kumar Giri  <br><b>PREPARED BY</b></td>\n        <td class=\"text-center\" style='padding: 0px !important'>Shyam Bihari Sharma  <br><b>FACTORY MANAGER</b></td>\n    </tr>\n</table>\n\n",
